<?xml version="1.0" encoding="UTF-8"?>
<program>
    <func name="Type test 2">
        <shell bg="1" cmd="notepad.exe" comment="run notepad"/>
        <wait ms="800"/>
        <focus title="Notepad" retries="25" interval_ms="200"/>
        <type mode="copy_paste" text="This is an example of text that was generated by UsefulClicker today. " />
    </func>

    <func name="Click hash demo">
        <!-- Use usefulcliker_gui to capture a rectangle in Notepad (e.g., a menu/tab) and paste the hex hash below -->
        <clickimg hash="REPLACE_WITH_HASH" threshold="8" button="left"/>
    </func>

    <call name="Type test 2"/>
    <wait ms="500"/>
    <call name="Click hash demo"/>
</program>
